{
	"name": "@necord/lavalink",
	"description": "A implementation of lavalink-client for Necord",
	"version": "1.0.1",
	"scripts": {
		"build": "rimraf dist && tsc -p tsconfig.build.json",
		"prepublish:npm": "npm run build",
		"publish:npm": "release-it",
		"prepublish:dev": "npm run build",
		"publish:dev": "npm publish --access public --tag dev",
		"prepare": "husky .github/husky",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "eslint --ignore-path .gitignore src/**/*.ts"
	},
	"lint-staged": {
		"*.ts": "npm run format"
	},
	"directories": {
		"lib": "src",
		"test": "test"
	},
	"publishConfig": {
		"access": "public"
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"keywords": [
		"necord",
		"lavalink",
		"lavalink-client",
		"music",
		"discord"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/necordjs/lavalink.git"
	},
	"bugs": {
		"url": "https://github.com/necordjs/lavalink"
	},
	"author": "Alexey Filippov <socket.someone@gmail.com>",
	"contributors": [
		"Alexey Filippov <socket.someone@gmail.com>"
	],
	"dependencies": {},
	"devDependencies": {
		"@commitlint/cli": "19.5.0",
		"@commitlint/config-angular": "19.5.0",
		"@favware/npm-deprecate": "1.0.7",
		"@nestjs/common": "10.4.4",
		"@nestjs/core": "10.4.4",
		"@nestjs/testing": "10.4.4",
		"@types/jest": "29.5.13",
		"@types/node": "22.6.1",
		"@typescript-eslint/eslint-plugin": "5.62.0",
		"@typescript-eslint/parser": "5.62.0",
		"discord.js": "^14.16.2",
		"eslint": "8.57.1",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-prettier": "5.2.1",
		"husky": "9.1.6",
		"jest": "29.7.0",
		"lavalink-client": "^2.3.3",
		"lint-staged": "15.2.10",
		"necord": "^6.8.6",
		"prettier": "3.3.3",
		"reflect-metadata": "0.2.2",
		"release-it": "17.6.0",
		"rimraf": "6.0.1",
		"rxjs": "7.8.1",
		"ts-jest": "29.2.5",
		"ts-node": "10.9.2",
		"typescript": "5.6.2"
	},
	"peerDependencies": {
		"@nestjs/common": "^10.2.0",
		"@nestjs/core": "^10.2.0",
		"discord.js": "^14.0.1",
		"necord": "^6.0.0",
		"lavalink-client": "^2.3.3",
		"reflect-metadata": "^0.2.1",
		"rxjs": "^7.2.0"
	}
}
